{
  "name": "tpl-react",
  "version": "0.0.1",
  "description": "Frontend starter with React, Redux, Parcel and friends",
  "license": "MIT",
  "homepage": "https://github.com/andreidmt/tpl-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreidmt/tpl-react.git"
  },
  "bugs": {
    "url": "https://github.com/andreidmt/tpl-react/issues"
  },
  "author": {
    "name": "Andrei Dumitrescu",
    "url": "https://github.com/andreidmt"
  },
  "scripts": {
    "----UTIL": "",
    "setup": "rm -rf ./node_modules && npm i && npm audit fix",
    "setup:ci": "npm ci && npm audit fix",
    "update": "npm-check --update",
    "----DEV": "",
    "start": "parcel serve src/index.html --no-autoinstall",
    "----BUILD": "",
    "build": "parcel build src/index.html",
    "----LINT": "",
    "lint:css": "stylelint \"src/*.css\" \"src/**/*.css\"",
    "lint:md": "markdownlint *.md",
    "lint:js": "eslint --ext .jsx,.js src",
    "lint": "npm run lint:md && npm run lint:css && npm run lint:js",
    "----TEST": "",
    "test": "riteway -r @babel/register -r ignore-styles src/**/*.test.js src/**/*.test.jsx | tap-nirvana",
    "tdd": "nodemon -e js,jsx,json --watch src --exec \"npm test\""
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@hot-loader/react-dom": "^16.13.0",
    "@mutant-ws/fetch-browser": "^1.4.0",
    "@mutant-ws/next": "^1.9.0",
    "@sentry/browser": "^5.24.2",
    "classnames": "^2.2.6",
    "debug": "^4.1.1",
    "fast-deep-equal": "^3.1.3",
    "just-a-list.redux": "^7.5.0",
    "luxon": "^1.25.0",
    "m.xyz": "^2.13.0",
    "qs": "^6.9.4",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "sanitize.css": "^12.0.1",
    "socket.io-client": "^2.3.0"
  },
  "alias": {
    "react-dom": "@hot-loader/react-dom",
    "core.ui": "./src/core.ui",
    "core.libs": "./src/core.libs",
    "core.hooks": "./src/core.hooks",
    "layout.base": "./src/layout.base"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-constant-elements": "^7.10.4",
    "@babel/plugin-transform-react-display-name": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config.xyz": "^5.13.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-no-inferred-method-name": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.1",
    "eslint-plugin-unicorn": "^22.0.0",
    "ignore-styles": "^5.0.1",
    "markdownlint-cli": "^0.23.2",
    "nodemon": "^2.0.4",
    "npm-check": "^5.9.2",
    "parcel": "^2.0.0-alpha.3.2",
    "postcss": "^8.1.0",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-global-import": "^1.0.6",
    "postcss-modules": "^3.2.2",
    "postcss-nested": "^4.2.3",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.1",
    "riteway": "^6.2.0",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "tap-nirvana": "^1.1.0"
  }
}
